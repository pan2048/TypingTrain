    <main><div class='container-fluid' id='dict_container'>
      <h1><span class='badge badge-primary'>string in Go</span></h1>
      <div class='container-fluid'>
          <pre id='word_content' style='white-space: pre-wrap;'>Go provides a large number of functions to work with strings in the string package.
&lt;pre&gt;&lt;code&gt;
package main

import (
	&quot;fmt&quot;
	s &quot;strings&quot;
)

var p = fmt.Println

func main() {
	p(&quot;Contains:  &quot;, s.Contains(&quot;test&quot;, &quot;es&quot;))
	p(&quot;Count:     &quot;, s.Count(&quot;test&quot;, &quot;t&quot;))
	p(&quot;HasPrefix: &quot;, s.HasPrefix(&quot;test&quot;, &quot;te&quot;))
	p(&quot;HasSuffix: &quot;, s.HasSuffix(&quot;test&quot;, &quot;st&quot;))
	p(&quot;Index:     &quot;, s.Index(&quot;test&quot;, &quot;e&quot;))
	p(&quot;Join:      &quot;, s.Join([]string{&quot;a&quot;, &quot;b&quot;}, &quot;-&quot;))
	p(&quot;Repeat:    &quot;, s.Repeat(&quot;a&quot;, 5))
	p(&quot;Replace:   &quot;, s.Replace(&quot;foo&quot;, &quot;o&quot;, &quot;0&quot;, -1))
	p(&quot;Replace:   &quot;, s.Replace(&quot;foo&quot;, &quot;o&quot;, &quot;0&quot;, 1))
	p(&quot;Split:     &quot;, s.Split(&quot;a-b-c-d-e&quot;, &quot;-&quot;))
	p(&quot;ToLower:   &quot;, s.ToLower(&quot;TEST&quot;))
	p(&quot;ToUpper:   &quot;, s.ToUpper(&quot;test&quot;))
	p()

	p(&quot;Len: &quot;, len(&quot;hello&quot;))
	p(&quot;Char:&quot;, &quot;hello&quot;[1])

	p([]byte(&quot;test&quot;))
	p(string([]byte{&#039;t&#039;, &#039;e&#039;, &#039;s&#039;, &#039;t&#039;}))
}
&lt;/code&gt;&lt;/pre&gt;</pre>
